<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_Change_CDNA_Nickname</fullName>
    <active>true</active>
    <errorConditionFormula>ISCHANGED(CommunityNickname) &amp;&amp;
PRIORVALUE(CommunityNickname) == 'CDNA'</errorConditionFormula>
    <errorDisplayField>Alias</errorDisplayField>
    <errorMessage>Nickname for Claude DNA cannot be changed. It is used in validation rules</errorMessage>
</ValidationRule>
