{!$Record.RT_Choice__c} != &quot;House/Villa Booking&quot;,
{!$Record.RT_Choice__c} != &quot;Tour Booking&quot;
),
IF(
VALUE({!$Record.Room_Rate__c}) * {!$Record.of_Nights__c} &gt; 0, 
{!getVendorDetails.Commission_Val__c} * (VALUE({!$Record.Room_Rate__c}) * {!$Record.of_Nights__c}) / 100,
