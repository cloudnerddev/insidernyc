AND(
{!$Record.RT_Choice__c} != &quot;Hotel Booking&quot;,
{!$Record.RT_Choice__c} != &quot;DMC / On Site Trip Booking&quot;,
{!$Record.RT_Choice__c} != &quot;Charter&quot;,
