                            </apex:outputtext> ·&nbsp;<apex:outputField value="{!logs.relatedLog.Check_Out_Time__c}">
                                <apex:param value="h:mm a" />
                            </apex:outputField>
                            <pre>Check Out</pre>
                        </td>
                        <td class="logThird">
                            <p>{!logs.relatedLog.of_Nights__c} Nights</p>
                        </td>
                    </tr>
                </table>
            </apex:outputPanel>  
            </div>
        <!-- </apex:OutputPanel> -->
        <!-- MIDDDLE -->
        <apex:outputText rendered="{!logs.relatedLog.Itinerary_Middle__c != ''}" value="{!logs.relatedLog.Itinerary_Middle__c}" escape="false"></apex:outputText>
        
