                                    <apex:param value="{!logs.relatedLog.Reconfirmation_Date_for_Email__c}"/>
                                </apex:outputText>
                        </td>
                    </tr>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!logs.relatedLog.Items_Ordered_Type_of_Arrangements_rich__c != ''}">
                    <tr>
                        <td valign="top">Items Ordered/Type of Arrangements:&nbsp;</td>
                        <td class="column2" valign="top">
                            <apex:outputText rendered="{!logs.relatedLog.Items_Ordered_Type_of_Arrangements_rich__c != ''}" value="{!logs.relatedLog.Items_Ordered_Type_of_Arrangements_rich__c}" escape="false"></apex:outputText>
                        </td>
                    </tr>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!logs.relatedLog.Return_Policy__c != ''}">
                    <tr>
                        <td valign="top">Return policy:&nbsp;</td>
                        <td class="column2" valign="top">
