                            <apex:outputField rendered="{!logs.relatedLog.Confirmed_With__c != ''}" value="{!logs.relatedLog.Confirmed_With__c}"/><apex:outputText value="{0, date, MMMM d','  yyyy}"><apex:param value="{!logs.relatedLog.Confirmation_Date_for_Email__c}"/></apex:outputText>
