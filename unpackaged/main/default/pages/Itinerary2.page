<apex:page controller="ItineraryTemplateController" renderAs="pdf" showChat="false" showHeader="false" applyHtmlTag="false" applyBodyTag="false">
    
    <head>
        
        <style> 
                body {
                    font-family: Arial, Helvetica, sans-serif;
                } 
                
                .mainHead{
                    text-align: center;
                }
                .col2{
                    text-align: right;
                }
                table {
                    text-items: center;
                    width: 100%
                }
                .logDate{
                    text-align: right;
                    width:30%;
                    position: relative;
                    padding-top:10px;
                    font-size: 9.7pt;
                }
                .logmid{
                    width:5%;
                    padding-top:10px;
                }
                .tableHr{
                    
                    padding-top:10px;
                    font-size: 9pt;
                    /* height: 70px;  */
                    /* text-align: left; */
                }
                .logHeading{
                    background: #267797;
                    color: white;
                    width: 100%;
                    padding: 1%;
                    margin-bottom: 2px;
                }
                .MainHeading{
                    background: #267797;
                    color: white;
                    width: 100%;
                    padding: 1%;
                    text-align: center;
                }
                .logFirst{
                    padding: 16px;
                    text-align: center;
                    
                    border-bottom: 1px solid #ddd;
                    border-top: 1px solid #ddd;
                    color:#267797;
                }
                .logSecond{
                    padding: 16px;
                    text-align: center;
                    border: 1px solid #ddd;
                    color:#267797;
                }
                .logThird{
                    padding: 16px;
                    valign: top;
                    text-align: center;
                    border-bottom: 1px solid #ddd;
                    border-top: 1px solid #ddd;
                    color:#267797;
                }
                .container {
                    position: relative;
                    color: white;
                    width: 100%;
                }
                .imgSize{
                    width: 800px;
                    height: 350px;
                    page-break-inside: avoid;
                }
                .logosize{
                    width: 160px;
                    height: 80px;
                }
                .spaceadd{
                    margin-bottom: 100px;
                }
                .column2{
                    width:65%;
                }
                .sfdc_richtext img {
                    margin-top: -18px;
                    height: 350px;
                    width: 800px;
                }
                .bottom-left {
                    position: absolute;
                    bottom: 0px;
                    left: 0px;
                    background-color: black;
                    width: 100%;
                    padding-left: 16px;
                    /* font-weight: bold; */
                    opacity: 0.5;
                    text-align: left;
                }
                .bottom-new {
                    bottom: 0px;
                    left: 0px;
                    background-color: black;
                    width: 100%;
                    padding-top: 5px;
                    padding-left: 16px;
                    padding-bottom: 5px;
                    /* font-weight: bold; */
                    text-align: left;
                }
                
                .line{
                    color:#267797;
                }
                .thikLine{
                    background-color: #267797;
                    width: 20%;
                    height: 4px;
                    margin-left: 40%;
                }
                .fontSize{
                    font-size: 9pt;
                }
                .imagesize{
                    width: 500px;
                    height: 200px;
                }
                .print:last-child {
                    page-break-after: auto;
                }
                .flightBooking {
                    size: letter;                    
                    font-family: monospace;
                    font-size: 14px;
                    text-align: left;
                }
                .page-break {
                    display:block;
                    page-break-after:always;
                }
                .colOne {
                    width: 40%;
                }
                .colTwo {
                    width: 40%;
                }
                .colThree {
                    width: 20%;
                }
                @media print {  
                  div.dontbreak {
                    display: grid;
                    page-break-inside: avoid;
                  }
                }
                .new-page-section {
                    page-break-before: always;
                }
        </style>
      </head>  
    <div>
        <table class="row">
            <tr>
                <td>
                    <div class="col1">
                        <apex:image styleClass="logosize" url="{!$Resource.LogoForItinerary}"/>
                    </div>
                </td>
                <td>
                    <div class="col2">
                        <pre>212.685.4113
                            24 hr:800.840.1143
                             24@insidernyc.com
                        </pre>
                    </div> 
                </td>
            </tr>
        </table>
    </div>
    <apex:outputpanel styleClass="imagesize" >
        <apex:outputField value="{!thisItinerary.Intro_Picture__c}" />
    </apex:outputpanel>
    <div class="MainHeading">
        <apex:outputtext value="{0, date, EEEE} · {0, date, MMMM d}, {0, date, yyyy}">
            <apex:param value="{!thisItinerary.Start_Date__c}"></apex:param>
        </apex:outputtext> - 
        <apex:outputtext value="{0, date, EEEE} · {0, date, MMMM d}, {0, date, yyyy}">
            <apex:param value="{!thisItinerary.End_Date__c}"></apex:param>
        </apex:outputtext>
    </div>
    <h2 class="mainHead">{!thisItinerary.name}</h2>
    <div  class="thikLine"></div>
    <table class="row">
        <apex:repeat value="{!headerList}" var="logs" id="theRepeat">
            <tr class="row">
                <td class="logDate">
                    <b>
                        <apex:outputtext value="{0, date, EEEE · MMMM d}">
                            <apex:param value="{!logs.Reference_Date}"></apex:param>
                        </apex:outputtext>
                    </b>
                </td>
                <td class="logmid"><p>  </p>
                </td>
                <td class="tableHr" >{!logs.subject}</td>
            </tr>
        </apex:repeat>
    </table>
    <div class="spaceadd"></div>
    <apex:repeat value="{!imgWrapper}" var="logs">
        <div class="dontbreak">
            <!--apex:outputPanel rendered="{!logs.isPageBreak == true}">
             <!--div style="page-break-after: always;"></div>
            </apex:outputPanel-->
            <!-- HEADER -->
            <apex:outputText rendered="{!logs.relatedLog.Itinerary_Header__c != ''}" value="{!logs.relatedLog.Itinerary_Header__c}" escape="false"></apex:outputText>
            <div class="logHeading">
                <apex:outputtext value="{0, date, EEEE · MMMM d}">
                    <apex:param value="{!logs.relatedLog.Reference_Date__c}"></apex:param>
                </apex:outputtext>
            </div>
             <div class="container" >
                <apex:outputPanel rendered="{!AND(logs.isStaticResource == true,logs.isParentImage  == false,logs.isChildImage == false)}">
                    <apex:image styleClass="imgSize" url="{!$Resource.CarRental}" rendered="{! logs.relatedLog.RecordTypeId == Car_Rental_RI }"/><!--Car Rental-->
                    <apex:image styleClass="imgSize" url="{!$Resource.CarService}" rendered="{!logs.relatedLog.RecordTypeId == Car_Service_RI}"/><!--Car Service-->
                    <apex:image styleClass="imgSize" url="{!$Resource.Charter}" rendered="{!logs.relatedLog.RecordTypeId == Charter_RI}"/><!--Charter-->
                    <apex:image styleClass="imgSize" url="{!$Resource.ItineraryDocTestImage}" rendered="{!logs.relatedLog.RecordTypeId == Event_Tickets_RI}"/><!--Event Tickets-->
                    <apex:outputPanel rendered="{!logs.relatedLog.RecordTypeId != Charter_RI}">
                        <div class="bottom-left">
                            <p>{!logs.relatedLog.Dynamic_Subject__c}<br />{!logs.relatedLog.Vendor_Phone__c}<br />{!logs.relatedLog.Vendor_Address__c}</p>
                        </div>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!logs.relatedLog.RecordTypeId == Charter_RI}">
                        <div class="bottom-left">
                            <p>{!logs.relatedLog.Dynamic_Subject__c}<br />{!logs.relatedLog.Vendor_Phone__c}<br />{!logs.relatedLog.Departure_Location_and_Address__c}</p>
                        </div>
                    </apex:outputPanel>
                    
                </apex:outputPanel>

                <apex:outputPanel rendered="{!AND(logs.isStaticResource == false,logs.isParentImage  == false,logs.isChildImage == true)}">
                    <apex:outputField value="{!logs.relatedLog.Itinerary_Image__c}" />
                    <apex:outputPanel rendered="{!logs.relatedLog.RecordTypeId != Tour_Booking_RI}"> 
                        <div class="bottom-left">
                            <p>{!logs.relatedLog.Dynamic_Subject__c}<br />{!logs.relatedLog.Vendor_Phone__c}<br />{!logs.relatedLog.Vendor_Address__c}</p>
                        </div>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!logs.relatedLog.RecordTypeId == Tour_Booking_RI}">
                        <div class="bottom-left">
                            <p>{!logs.relatedLog.Dynamic_Subject__c}<br />{!logs.relatedLog.Vendor_Phone__c}<br />{!logs.relatedLog.Meeting_Location__c}</p>
                        </div>
                    </apex:outputPanel>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!AND(logs.isStaticResource == false,logs.isParentImage  == true,logs.isChildImage == false)}">
                    <apex:outputField value="{!logs.relatedLog.Vendor__r.Itinerary_Image__c}"/>
                    <apex:outputPanel rendered="{!logs.relatedLog.RecordTypeId != Tour_Booking_RI}"> 
                        <div class="bottom-left">
                            <p>{!logs.relatedLog.Dynamic_Subject__c}<br />{!logs.relatedLog.Vendor_Phone__c}<br />{!logs.relatedLog.Vendor_Address__c}</p>
                        </div>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!logs.relatedLog.RecordTypeId == Tour_Booking_RI}">
                        <div class="bottom-left">
                            <p>{!logs.relatedLog.Dynamic_Subject__c}<br />{!logs.relatedLog.Vendor_Phone__c}<br />{!logs.relatedLog.Meeting_Location__c}</p>
                        </div>
                    </apex:outputPanel>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!AND(logs.isStaticResource == false,logs.isParentImage  == false,logs.isChildImage == false)}">
                    <div class="bottom-new">
                        <p>{!logs.relatedLog.Dynamic_Subject__c}<br />{!logs.relatedLog.Vendor_Phone__c}<br />{!logs.relatedLog.Vendor_Address__c}</p>
                    </div>
                </apex:outputPanel>
            </div>
        </div>
         
        <!-- <apex:OutputPanel rendered="{!logs.relatedFlight == Null}"> -->
            <div>
            <apex:OutputPanel rendered="{!IF(OR(logs.relatedLog.RecordTypeId == Hotel_Booking_RI,logs.relatedLog.RecordTypeId == House_Villa_Booking_RI),true,false)}">
                <table class="row" >
                    <tr>
                        <td class="logFirst">
                            <apex:outputtext value="{0, date, MMM d}">
                                <apex:param value="{!logs.relatedLog.Check_In_Date__c}"></apex:param>
                            </apex:outputtext> ·&nbsp;
                            <apex:outputField value="{!logs.relatedLog.Check_In_Time__c}">
                                <apex:param value="h:mm a" />
                            </apex:outputField> 
                            <pre>Check In</pre>
                        </td>
                        <td class="logSecond">
